{
  "name": "@sensethenlove/svelte-loading-anchor",
  "version": "1.0.2",
  "author": "Sense Then Love",
  "license": "MIT",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/sensethenlove/svelte-loading-anchor#readme",
  "description": "Svelte component that supports an href property, shows a loading icon when clicked & stops loading on page navigate",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensethenlove/svelte-loading-anchor.git"
  },
  "bugs": {
    "url": "https://github.com/sensethenlove/svelte-loading-anchor/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "link",
    "loading",
    "typescript",
    "cloudflare",
    "cloudflare workers"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm check && pnpm package",
    "live": "pnpm build && npm publish --access public",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "sass": "^1.60.0"
  }
}
